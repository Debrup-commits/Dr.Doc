# Pure Fetch.ai ASI:One Configuration
# This configuration uses ONLY Fetch.ai models - no OpenAI required!

# Fetch.ai ASI:One API Key (REQUIRED)
ASI_ONE_API_KEY=your_asi_one_api_key_here

# Model Selection (true = use Fetch.ai ASI:One, false = use OpenAI)
USE_FETCHAI=true

# OpenAI Fallback (false = pure Fetch.ai, no OpenAI fallback)
REQUIRE_OPENAI_FALLBACK=false

# MeTTa Integration (set to true to enable MeTTa - requires hyperon package)
# Note: MeTTa is optional and system works fine without it
ENABLE_METTA=false

# Optional: Customize model settings
# FETCHAI_CHAT_MODEL=gpt-4o-mini
# FETCHAI_EMBEDDING_MODEL=text-embedding-3-small

# Optional: Customize vector store settings
# VECTOR_STORE_PATH=../vector_store
# ATOMS_FILE=../api_facts.metta
# DOCS_DIR=../docs

# Optional: Flask settings
# FLASK_DEBUG=False
# RAG_PORT=5000
# API_PORT=5001
# FETCH_AGENT_PORT=8001
# FETCHAI_HYBRID_PORT=5002

# Optional: Debug settings
# DEBUG_MODE=false

